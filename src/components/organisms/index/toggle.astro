<body id="page-top">
  <!-- Page Wrapper -->
  <div id="wrapper" class="pt-20 mt-10 mb-10  align-center items-center h-auto bg-bgdark">
	
    <!-- Content Wrapper -->
    <div id="content-wrapper" class="container d-flex flex-column">
      <!-- Main Content -->
      <div id="content">
        <!-- Begin Page Content -->
        <div class="container-fluid">
          <div class="tabControl">
            <div class="container">
              <ul class="nav nav-pills font-grotesk color-textblue font-bold nav-fill" id="pills-tab" role="tablist">
                <li class="nav-item">
                  <a class="nav-link active " id="pills-home-tab" data-toggle="pill" href="#pills-1" role="tab">{t("index.toggle.Defination")}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="pills-profile-tab" data-toggle="pill" href="#pills-2" role="tab">{t("index.toggle.Features")}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#pills-3" role="tab">{t("index.toggle.Theme")}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#pills-4" role="tab">{t("index.toggle.Technology Breakthrough")}</a>
                </li>
                <li class="nav-item">
                  <a class="nav-link" id="pills-contact-tab" data-toggle="pill" href="#pills-5" role="tab">{t("index.toggle.Biggest Innovation")}</a>
                </li>
              </ul>
            </div>
            <div class="tab-content" id="pills-tabContent">
              <div class="tab-pane show active" id="pills-1" role="tabpanel" aria-labelledby="pills-home-tab">
                
                <Togglecardlayer body1= {t("index.toggle.toggleCard1.body1")}
                                body2= {t("index.toggle.toggleCard1.body2")}
                                body3= {t("index.toggle.toggleCard1.body3")}
                                body4= {t("index.toggle.toggleCard1.body4")}/>
                
              </div>
              <div class="tab-pane fade" id="pills-2" role="tabpanel" aria-labelledby="pills-profile-tab">
                
                <Togglecardlayer2 />

              </div>
              <div class="tab-pane fade" id="pills-3" role="tabpanel" aria-labelledby="pills-contact-tab">
                
                <Togglecardlayer body1= {t("index.toggle.toggleCard2.body1")}
                                body2= {t("index.toggle.toggleCard2.body2")}
                                body3= {t("index.toggle.toggleCard2.body3")}
                                body4= {t("index.toggle.toggleCard2.body4")}/>

              </div>
              <div class="tab-pane fade" id="pills-4" role="tabpanel" aria-labelledby="pills-contact-tab">
                <Togglecardlayer body1= {t("index.toggle.toggleCard3.body1")}
                                body2= {t("index.toggle.toggleCard3.body2")}
                                body3= {t("index.toggle.toggleCard3.body3")}
                                body4= {t("index.toggle.toggleCard3.body4")}/>

              </div>
              <div class="tab-pane fade" id="pills-5" role="tabpanel" aria-labelledby="pills-contact-tab">
                <Togglecardlayer body1= {t("index.toggle.toggleCard4.body1")}
                                body2= {t("index.toggle.toggleCard4.body1")}
                                body3= {t("index.toggle.toggleCard4.body1")}
                                body4= {t("index.toggle.toggleCard4.body1")}/>

              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
